{
  "name": "durable-cf-streams",
  "version": "0.1.41",
  "description": "pure building blocks for durable streams on cloudflare",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/flbn/durable-cf-streams.git",
    "directory": "packages/durable-cf-streams"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "durable-objects",
    "streams",
    "d1",
    "kv",
    "r2"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./storage": {
      "types": "./dist/storage/index.d.ts",
      "import": "./dist/storage/index.js"
    },
    "./storage/memory": {
      "types": "./dist/storage/memory.d.ts",
      "import": "./dist/storage/memory.js"
    },
    "./storage/d1": {
      "types": "./dist/storage/d1.d.ts",
      "import": "./dist/storage/d1.js"
    },
    "./storage/kv": {
      "types": "./dist/storage/kv.d.ts",
      "import": "./dist/storage/kv.js"
    },
    "./storage/r2": {
      "types": "./dist/storage/r2.d.ts",
      "import": "./dist/storage/r2.js"
    },
    "./storage/sqlite": {
      "types": "./dist/storage/sqlite.d.ts",
      "import": "./dist/storage/sqlite.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22"
  },
  "volta": {
    "node": "22.16.0"
  },
  "scripts": {
    "build": "tsup",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "effect": "^3.19.13"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251229.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {}
}
